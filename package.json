{
  "name": "paperworkiq-ai-app",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@9.12.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm -C backend dev\" \"pnpm -C frontend dev\"",
    "dev:backend": "pnpm -C backend dev",
    "dev:frontend": "pnpm -C frontend dev",
    "build": "pnpm -C backend build && pnpm -C frontend build",
    "lint": "pnpm -C backend lint && pnpm -C frontend lint"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
